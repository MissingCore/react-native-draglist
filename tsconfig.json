{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./dist",
    "target": "ESNext",
    "strict": true,
    "jsx": "react-native",
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    // "noUncheckedIndexedAccess": true,
    "verbatimModuleSyntax": true,
    "noEmit": false,
    "types": []
  },
  "include": ["src/**/*"],
  "exclude": ["**/__tests__/*"]
}
